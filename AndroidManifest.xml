<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.andromeda"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="18" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.andromeda.MainActivity"
            android:label="@string/app_name"
            android:screenOrientation="portrait"
            android:theme="@style/CustomActionBarTheme" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.andromeda.Start_to_play"
            android:label="@string/title_activity_start_to_play"
            android:parentActivityName="com.example.aktiviti.MainActivity"
            android:screenOrientation="portrait"
            android:theme="@style/CustomActionBarTheme" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.aktiviti.MainActivity" />
        </activity>
        <activity
            android:name="com.example.andromeda.Build"
            android:label="@string/title_activity_build"
            android:parentActivityName="com.example.andromeda.Start_to_play"
            android:screenOrientation="portrait"
            android:theme="@style/CustomActionBarTheme" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.andromeda.Start_to_play" />
        </activity>
        <activity
            android:name="com.example.andromeda.Rudnik_titana"
            android:label="@string/title_activity_rudnik_titana"
            android:screenOrientation="portrait"
            android:theme="@style/CustomActionBarTheme" > >
        </activity>
    </application>

</manifest>
